<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stadiumreservation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StadiumReservation">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Information" android:label="Information"></activity>
        <activity android:name=".LoginActivity" android:label="LoginActivity"></activity>
        <activity android:name=".MainMenu" android:label="MainMenu"></activity>
        <activity android:name=".Reservation" android:label="Reservation"></activity>
        <activity android:name=".Search" android:label="Search"></activity>
        <activity android:name=".MyMatch" android:label="MyMatch"></activity>
        <activity android:name=".Apply" android:label="Apply"></activity>
        <activity android:name=".ReservationInfor" android:label="ReservationInfo"></activity>
    </application>

</manifest>