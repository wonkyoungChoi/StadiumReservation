<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stadiumreservation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StadiumReservation">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Information" android:label="Information"/>
        <activity android:name=".LoginActivity" android:label="LoginActivity"/>
        <activity android:name=".MainMenu" android:label="MainMenu"/>
        <activity android:name=".Reservation" android:label="Reservation"/>
        <activity android:name=".Search" android:label="Search" />
        <activity android:name=".MyMatch" android:label="MyMatch" />
        <activity android:name=".Apply" android:label="Apply" />
        <activity android:name=".ReservationInfo" android:label="ReservationInfo" />
        <activity android:name=".ApplyInfo" android:label="ApplyInfo" />
    </application>

</manifest>